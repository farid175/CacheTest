@model List<string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form asp-controller="SortedSetType" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit">Add</button>
</form>



@foreach (var item in Model)
{
    <table class="table-success">
        <thead>
            <tr>
                <th>Name</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@item</td>
                <td><a asp-controller="SortedSetType" asp-action="DeleteItem" asp-route-name="@item">Sil</a></td>
            </tr>
        </tbody>
    </table>
}

